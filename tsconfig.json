{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,  
    "outDir": "../../build",  
    "rootDir": "../../",              
    "importHelpers": true,  
    "strict": true,    
    "moduleResolution": "node", 
    "baseUrl": "../../",                                                    
    "esModuleInterop": true,                
    "skipLibCheck": true,   
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "paths": {
      "Elucidate/*":["./node_modules/expresswebcorets/lib/*"],
    }
  }
}
